{
  "name": "theupto",
  "version": "0.9.0",
  "description": "A multi-user responsive real-time web app for sharing, arranging, and transforming images collaboratively on your laptop or phone.",
  "author": "Andrew Nease <andrew.nease.code@gmail.com",
  "private": true,
  "scripts": {
    "dev-build": "npm-run-all --parallel mongo:run server:watch build:watch",
    "prod-build": "npm-run-all --parallel build server:run",
    "build:watch": "NODE_ENV=development webpack --progress --watch",
    "build": "NODE_ENV=production webpack",
    "server:watch": "PORT=8080 nodemon app.js",
    "server:run": "PORT=80 node app.js",
    "mongo:run": "mongod --quiet",
    "deploy-local": "npx shipit production deploy-local",
    "deploy-master": "npm-run-all deploy restart-server-current",
    "deploy": "npx shipit production deploy",
    "restart-server-current": "npx shipit production restart-server-current"
  },
  "dependencies": {
    "aws-sdk": "^2.54.0",
    "body-parser": "^1.0.0",
    "busboy": "^0.2.12",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.2",
    "mongoose": "^4.0.0",
    "npm-run-all": "^4.0.2",
    "passport": "^0.3.2",
    "randomstring": "^1.1.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-image": "^1.0.1",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "s3-stream-upload": "^2.0.1",
    "serve-favicon": "^2.3.0",
    "socket.io": "^2.0.2",
    "uniqid": "^4.1.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "false": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "shipit-cli": "^4.1.1",
    "shipit-deploy": "^4.1.1",
    "style-loader": "^0.19.1"
  }
}
